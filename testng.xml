    </test>

    <test name="Check currency">
        <classes>
            <class name="study.alevel.tests.serchandfilter.CurrencyChange"/>
        </classes>
    </test>

    <test name="SearchAdd">
        <parameter name="search" value="Тенісна ракетка"/>
        <parameter name="check" value="теніс,ракетка,теннис"/>
        <classes>
            <class name="study.alevel.tests.serchandfilter.SearchAdd"/>
        </classes>
    </test>

    <test name="PriceFilter">
        <classes>
            <class name="study.alevel.tests.serchandfilter.PriceFilterTest"/>
