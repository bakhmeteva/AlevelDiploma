<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Test suite">
    <test name="Blog footer btn ">
        <classes>
            <class name="study.alevel.tests.footertests.BlogTest"/>
        </classes>
    </test>

    <test name="MobileApps footer btn">
        <classes>
            <class name="study.alevel.tests.footertests.MobileApps"/>
        </classes>
    </test>

    <test name="Check all categories">
        <classes>
            <class name="study.alevel.tests.mainfunc.CategoriesCheck"/>
        </classes>
