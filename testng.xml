    </test>

    <test name="Change language to russian">
        <classes>
            <class name="study.alevel.tests.mainfunc.ChangeLangToRussian"/>
        </classes>
    </test>

    <test name="Login negative test">
        <classes>
            <class name="study.alevel.tests.mainfunc.LoginNegative"/>
        </classes>
    </test>

    <test name="Login">
        <classes>
            <class name="study.alevel.tests.mainfunc.LoginUser"/>
        </classes>
